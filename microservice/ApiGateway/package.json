{
  "name": "parkwise-api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for ParkWise external integrations",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "vitest run --no-file-parallelism --testTimeout=3000 --coverage"
  },
  "dependencies": {
    "cors": "*",
    "express": "*",
    "express-rate-limit": "*",
    "helmet": "*",
    "node-fetch": "*",
    "typescript-eslint": "^8.33.1"
  },
  "devDependencies": {
    "@types/cors": "*",
    "@types/express": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/node-fetch": "*",
    "@types/supertest": "*",
    "@vitest/coverage-v8": "*",
    "eslint": "*",
    "msw": "*",
    "nodemon": "*",
    "supertest": "*",
    "ts-node": "*",
    "typescript": "*",
    "vitest": "*"
  },
  "engines": {
    "node": "*"
  }
}
